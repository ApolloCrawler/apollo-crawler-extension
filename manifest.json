{
    "manifest_version": 2,

    "name": "Apollo Crawler",
    "version": "0.0.2",
    "description": "Apollo Crawler In-Browser Extractor Engine",

    "icons": {
        "128": "icon-128.png"
    },

    "browser_action": {
        "default_icon": "icon-128.png"
    },

    "background": {
        "page": "background.html"
    },

    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],

    "web_accessible_resources": [
        "contentscript.js"
    ]
}
